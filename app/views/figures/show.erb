<table>
  <tr>
    <th>Name</th>
    <th>Landmarks</th>
    <th>Titles</th>
  </tr>
  <tr>
    <td><%= @figure.name %></td>
    <td>
      <ul>
        <% @figure.landmarks.each do |landmark| %>
          <li><%=landmark.name%></li>
        <% end %>
      </ul>
    </td>
    <td>
      <ul>
        <% @figure.titles.each do |title| %>
          <li><%=title.name%></li>
        <% end %>
      </ul>
    </td>
  </tr>
<table>
